{
  "additionalBranchPrefix": "{{parentDir}}-",
  "automerge": true,
  "automergeType": "branch",
  "extends": [
    "config:base",
    "group:allNonMajor",
    "schedule:monthly",
    ":pinDependencies",
    "group:typescript-eslintMonorepo"
  ],
  "labels": ["chore", "dependencies"],
  "semanticCommits": "enabled",
  "separateMajorMinor": false,
  "timezone": "Europe/Berlin",
  "vulnerabilityAlerts": {
    "labels": ["security"]
  }
}
